{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "SampleConfig": {
      "type": "object",
      "properties": {
        "key": {
          "type": "string"
        }
      },
      "required": [
        "key"
      ]
    },
    "Aliases": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "String": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "sample": {
          "$ref": "#definitions/SampleConfig"
        },
        "aliases": {
          "$ref": "#definitions/Aliases"
        },
        "apiKey": {
          "type": "string"
        }
      },
      "required": [
        "title",
        "sample",
        "aliases",
        "apiKey"
      ]
    },
    "KBaseSchemaConfig": {
      "type": "object",
      "properties": {
        "sample": {
          "type": "object",
          "properties": {
            "key": {
              "type": "string"
            },
            "unit": {
              "type": "string"
            },
            "formatting": {
              "type": "object",
              "properties": {
                "minimumFractionDigits": {
                  "type": "number"
                },
                "maximumFractionDigits": {
                  "type": "number"
                }
              }
            }
          },
          "required": [
            "key"
          ]
        }
      }
    }
  }
}